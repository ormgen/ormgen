{
	"name": "ormgen",
	"version": "0.0.7",
	"main": "dist/index.js",
	"license": "MIT",
	"packageManager": "yarn@4.1.1",
	"files": [
		"dist",
		"setup.js"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"postinstall": "node setup.js",
		"dev": "tsup-node --watch",
		"build": "tsup-node",
		"deploy": "npm run build && npm publish",
		"deploy:patch": "npm version patch && npm run deploy",
		"test:types": "tsc --noEmit",
		"test:types:watch": "tsc --noEmit --watch"
	},
	"devDependencies": {
		"@prisma/client": "^5.11.0",
		"@types/node": "^20.11.28",
		"fast-glob": "^3.3.2",
		"prettier": "^3.2.5",
		"tsup": "^8.0.2",
		"typescript": "^5.4.2",
		"zod": "^3.22.4"
	},
	"dependencies": {
		"@amono/find-up": "^0.0.5",
		"case-anything": "^2.1.13"
	},
	"peerDependencies": {
		"@prisma/client": "*"
	}
}
